{
  "name": "vdm-browser-bridge",
  "version": "0.0.1",
  "description": "It provides your browser with VDM-Compatibility together with a connector program.",
  "scripts": {
    "init": "(cd app/scripts; bower install crypto-js)",
    "dev": "webextension-toolbox dev",
    "build": "webextension-toolbox build",
    "sign": "web-ext sign --api-key=$npm_package_config_jwt_issuer --api-secret=$npm_package_config_jwt_sign -a packages -s dist/firefox"
  },
  "devDependencies": {
    "web-ext": "^7.2.0",
    "webextension-toolbox": "latest"
  }
}
